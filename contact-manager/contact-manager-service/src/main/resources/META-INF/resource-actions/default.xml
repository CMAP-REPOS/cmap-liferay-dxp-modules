<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action  
Mapping 7.0.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_0_0.dtd">
<resource-action-mapping>
	<model-resource>
	    <model-name>contact.manager.model.CrmContact</model-name>
	    <portlet-ref>
	        <portlet-name>ContactManagerApp</portlet-name>
	        <portlet-name>GroupManagerApp</portlet-name>
	        <portlet-name>RecyclingBinApp</portlet-name>
	    </portlet-ref>
	    <root>true</root>
		<permissions>
			<supports>
				<action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>VIEW</action-key>
	        </supports>
	        <site-member-defaults>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>VIEW</action-key>
	        </site-member-defaults>
	        <guest-defaults>
	            <action-key>VIEW</action-key>
	        </guest-defaults>
	        <guest-unsupported>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	        </guest-unsupported>
	    </permissions>
	</model-resource>
</resource-action-mapping>