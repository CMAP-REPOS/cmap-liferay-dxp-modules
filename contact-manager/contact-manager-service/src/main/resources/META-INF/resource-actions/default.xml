<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action  
Mapping 7.0.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_0_0.dtd">
<resource-action-mapping>
	<model-resource>
	    <model-name>contact.manager.model.CrmContact</model-name>
	    <portlet-ref>
	        <portlet-name>ContactManagerApp</portlet-name>
	    </portlet-ref>
	    <permissions>
	        <supports>
	            <action-key>ACCESS_IN_CONTROL_PANEL</action-key>
				<action-key>VIEW</action-key>
				<action-key>ADD_CONTACT</action-key>
				<action-key>ADD_CONTACT_NOTE</action-key>
				<action-key>ADD_OUTREACH_NOTE</action-key>
				<action-key>MODIFY_CONTACT</action-key>
				<action-key>MODIFY_CONTACT_NOTE</action-key>
				<action-key>DELETE_CONTACT</action-key>
				<action-key>DELETE_CONTACT_NOTE</action-key>
				<action-key>DELETE_OUTREACH_NOTE</action-key>
				<action-key>VIEW_CONTACTS</action-key>
				<action-key>VIEW_CONTACT_NOTES</action-key>
	        </supports>
	        <site-member-defaults />
			<guest-defaults />
			<guest-unsupported>
				<action-key>ACCESS_IN_CONTROL_PANEL</action-key>
				<action-key>VIEW</action-key>
				<action-key>ADD_CONTACT</action-key>
				<action-key>ADD_CONTACT_NOTE</action-key>
				<action-key>ADD_OUTREACH_NOTE</action-key>
				<action-key>MODIFY_CONTACT</action-key>
				<action-key>MODIFY_CONTACT_NOTE</action-key>
				<action-key>DELETE_CONTACT</action-key>
				<action-key>DELETE_CONTACT_NOTE</action-key>
				<action-key>DELETE_OUTREACH_NOTE</action-key>
				<action-key>VIEW_CONTACTS</action-key>
				<action-key>VIEW_CONTACT_NOTES</action-key>
			</guest-unsupported>
	    </permissions>
	</model-resource>
	<model-resource>
	    <model-name>contact.manager.model.CrmGroup</model-name>
	    <portlet-ref>
	        <portlet-name>GroupManagerApp</portlet-name>
	    </portlet-ref>
	    <permissions>
	        <supports>
	            <action-key>ACCESS_IN_CONTROL_PANEL</action-key>
				<action-key>VIEW</action-key>
				<action-key>ADD_GROUP</action-key>
				<action-key>MODIFY_GROUP</action-key>
				<action-key>DELETE_GROUP</action-key>
				<action-key>VIEW_GROUPS</action-key>
	        </supports>
	        <site-member-defaults />
			<guest-defaults />
			<guest-unsupported>
				<action-key>ACCESS_IN_CONTROL_PANEL</action-key>
				<action-key>VIEW</action-key>
				<action-key>ADD_GROUP</action-key>
				<action-key>MODIFY_GROUP</action-key>
				<action-key>DELETE_GROUP</action-key>
				<action-key>VIEW_GROUPS</action-key>
			</guest-unsupported>
	    </permissions>
	</model-resource>
</resource-action-mapping>